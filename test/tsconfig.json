{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": ["es2016", "dom"],
    "rootDir": "../",
    "moduleResolution": "node",
    "outDir": "../dist/js",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
