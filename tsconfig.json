{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["es2017", "dom"],
    "module": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./dist/js/build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  },
  "typedocOptions": {
    "entryPoints": [
      "src/ts/index.ts",
      "src/ts/ana.ts",
      "src/ts/elements.ts",
      "src/ts/standard.ts",
    ],
    "out": "docs"
  }
}
