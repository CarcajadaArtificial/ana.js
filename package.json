{
  "name": "ana.js",
  "version": "0.5.0",
  "description": "Anarchy",
  "main": "gulpfile.js",
  "scripts": {
    "_build-sass": "sass ./src/scss/style.scss ./dist/ana/ana.css",
    "_build-sass:doc": "mkdir ./dist/sassdoc; sassdoc src/ --parse > ./dist/sassdoc/sassdoc.json",
    "_build-sass:min": "sass --style=compressed --no-source-map ./src/scss/style.scss ./dist/css/style.min.css",
    "_build-ts": "webpack --mode development",
    "_build-ts:doc": "npx typedoc --out ./dist/typedoc --json ./dist/typedoc/typedoc.json",
    "_publish-ts": "webpack --mode production",
    "test": "nodemon -e ts,scss,html test/app.ts",
    "build": "npm run _build-sass; npm run _build-ts;",
    "doc": "npm run _build-sass:doc; npm run _build-ts:doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CarcajadaArtificial/ana.js.git"
  },
  "author": "Oscar Alfonso Guerrero Nu√±ez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CarcajadaArtificial/ana.js/issues"
  },
  "homepage": "https://github.com/CarcajadaArtificial/ana.js#readme",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.17",
    "babel-loader": "^8.2.3",
    "nodemon": "^2.0.15",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "express": "^4.17.2",
    "html-element": "^2.3.1"
  }
}
