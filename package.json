{
  "name": "ana.js",
  "version": "0.6.13",
  "description": "A JavaScript library for the cool kids.",
  "author": "Oscar Alfonso Guerrero Nu√±ez",
  "license": "MIT",
  "scripts": {
    "dev": "npm run build; vite",
    "dev-css": "sass ./src/scss/style.scss ./dist/ana.css --watch",
    "build": "tsc && vite build; npm run build-css",
    "build-css": "sass --style=compressed --no-source-map ./src/scss/style.scss ./dist/ana.css",
    "preview": "vite preview"
  },
  "devDependencies": {
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vite-plugin-dts": "^1.2.0"
  },
  "dependencies": {
    "classnames": "^2.3.1"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/ana.umd.js",
  "types": "./dist/ts/index.d.ts",
  "module": "./dist/ana.es.js",
  "exports": {
    ".": {
      "import": "./dist/ana.es.js",
      "require": "./dist/ana.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CarcajadaArtificial/ana.js.git"
  },
  "bugs": {
    "url": "https://github.com/CarcajadaArtificial/ana.js/issues"
  },
  "homepage": "https://github.com/CarcajadaArtificial/ana.js#readme"
}
